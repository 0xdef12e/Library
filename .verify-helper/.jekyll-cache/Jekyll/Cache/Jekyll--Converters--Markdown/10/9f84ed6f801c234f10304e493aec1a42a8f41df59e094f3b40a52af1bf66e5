			<span class="k">for</span><span class="p">(</span><span class="n">q</span><span class="o">=</span><span class="n">x</span><span class="o">-&gt;</span><span class="n">lch</span><span class="p">;</span><span class="n">q</span><span class="o">-&gt;</span><span class="n">rch</span><span class="o">-&gt;</span><span class="n">rch</span><span class="o">!=</span><span class="nb">NULL</span><span class="p">;</span><span class="n">q</span><span class="o">=</span><span class="n">q</span><span class="o">-&gt;</span><span class="n">rch</span><span class="p">);</span>
